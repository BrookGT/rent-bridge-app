{
    "expo": {
        "name": "rent-bridge-app",
        "slug": "rent-bridge-app",
        "version": "1.0.0",
        "orientation": "default",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "backgroundColor": "#ffffff"
            }
        },
        "web": {
            "bundler": "metro"
        },
        "plugins": [
            [
                "expo-splash-screen",
                {
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#ffffff"
                }
            ]
        ]
    }
}
